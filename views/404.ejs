        <li><a href="discover">Discover</a></li>
        <li><a href="meet">Meet</a></li>
        <li><a href="/">About</a></li>
      </ul>
      <button class="mobile"><i class="fa fa-bars"></i></button>
      <div class="profile">
        <% if (isAuthenticated) { %>
          <!-- Green circle for authenticated -->
          <div class="circle green"></div>
        <% } else { %>
